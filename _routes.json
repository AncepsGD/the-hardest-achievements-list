{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/styles.css"],
  "routes": [
    {
      "pattern": "/achievement/*",
      "serve": "/achievement.html"
    },
    {
      "pattern": "/list",
      "serve": "/list.html"
    },
    {
      "pattern": "/404",
      "serve": "/404.html"
    },
    {
      "pattern": "/about-us",
      "serve": "/about-us.html"
    },
    {
      "pattern": "/leaderboard",
      "serve": "/leaderboard.html"
    },
    {
      "pattern": "/submission-stats",
      "serve": "/submission-stats.html"
    },
    {
      "pattern": "/timeline",
      "serve": "/timeline.html"
    }
  ]
}
